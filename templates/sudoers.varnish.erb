
## This part comes from modules/varnish/manifests/classes/administration.pp
<% if has_variable?("sudo_varnish_admin_user") -%>
User_Alias VARNISH_ADMIN = %varnish-admin,<%= sudo_varnish_admin_user %>
<% else -%>
User_Alias VARNISH_ADMIN = %varnish-admin
<% end -%>
<% if has_variable?("sudo_varnish_admin_cmnd") -%>
Cmnd_Alias VARNISH_ADMIN = <%= sudo_varnish_admin_cmnd %>
<% else -%>
Cmnd_Alias VARNISH_ADMIN = /etc/init.d/varnish*
<% end -%>
VARNISH_ADMIN ALL=(root) VARNISH_ADMIN
